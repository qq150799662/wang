    function getIP2(json) {
        //console.log(JSON.stringify(json));
        localStorage.setItem("ip222", json.ip);
    }
    function getIP3(json) {
        //console.log(JSON.stringify(json));
        localStorage.setItem("ip333", json.query);
    }
    function getIP4(json) {
        //console.log(JSON.stringify(json));
        localStorage.setItem("ip444", json.ip);
    }
</script>
